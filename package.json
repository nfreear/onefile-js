{
  "name": "otu",
  "x-name": "onefile",
  "description": "One-file npm-package Javascripts. Inspired by php-melody — 'One-file composer scripts'.",
  "version": "1.2.0-beta",
  "license": "MIT",
  "main": "index",
  "bin": {
    "otu": "bin/otu.js"
  },
  "x-otu-directory": "~/.otu-js/",
  "preferGlobal": true,
  "author": "Nick Freear {@nfreear}",
  "repository": "https://github.com/nfreear/onefile-js.git",
  "dependencies": {
    "espree": "^3.5.1",
    "js-yaml": "^3.10.0",
    "shelljs": "^0.7.8",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "semistandard": "^11.0.0"
  },
  "scripts": {
    "otu": "bin/otu.js run examples/shelljs-ex.js",
    "semi": "semistandard",
    "test": "semistandard && bin/otu.js example"
  },
  "keywords": [ "melody-php", "npm-package", "es6", "espree", "cli", "one-file" ],
  "x-melody-url": "http://melody.sensiolabs.org/",
  "x-copyright": "© Nick Freear, 20-Sep-2017."
